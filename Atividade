Algoritimo 1º seleção:

package main

import "fmt"

func main() {
	var x int
	var y int
	fmt.Print("Digite o 1º número")
	fmt.Scan(&x)
	fmt.Print("Digite o 2º número")
	fmt.Scan(&y)
	if x < y {
		fmt.Println("O maior número é", y)
	} else {
		fmt.Println("O maior número é", x)
	}
}

Algoritimo 2º seleção:

package main

import "fmt"

func main() {
	var x int
	var y int
	var z int
	fmt.Print("Digite o 1º número")
	fmt.Scan(&x)
	fmt.Print("Digite o 2º número")
	fmt.Scan(&y)
	fmt.Print("Digite o 3º número")
	fmt.Scan(&z)
	if x < y && x < z {
		fmt.Println("O menor número é", x)
	} else if y < x && z > y {
		fmt.Println("O menor número é", y)
	} else {
		fmt.Println("O menor número é", z)
	}
}

algoritimo 3º seleção:

package main

import "fmt"

func main() {
	var i int
	fmt.Print("Digite um número!")
	fmt.Scan(&i)
	if i%2 == 0 {
		fmt.Println("Número ", i, " é par")
	} else {
		fmt.Println("Número ", i, " é impar")
	}

}

Algoritimo 4º seleção:

package main

import "fmt"

func main() {
	var x float64
	var y float64
	var z string
	fmt.Print("Digite sua altura! ")
	fmt.Scan(&x)
	fmt.Print("Digite seu peso! ")
	fmt.Scan(&y)
	fmt.Print("Digite o seu sexo! ")
	fmt.Scan(&z)
	fmt.Println("O seu iMC é", y/(x*x))
	if y/(x*x) <= 25 {
		fmt.Println("O seu peso é ideal")
	} else {
		fmt.Println("Você está acima do peso")
	}

}

Algoritimo 6º seleção:

package main

import "fmt"

func main() {
	var x int64
	var y int64
	fmt.Print("Digite um número! ")
	fmt.Scan(&x)
	fmt.Print("Digite outro número! ")
	fmt.Scan(&y)
	fmt.Println("a multiplicação entre seus números é de: ", x*y)
	if x*y > 0 {
		fmt.Println("Seu número é positivo")
	} else {
		fmt.Println("Seu número é negativo")
	}

}

Algoritimo 7º seleção:

package main

import "fmt"

func main() {
	var x float64
	fmt.Print("Digite o seu salário! ")
	fmt.Scan(&x)
	if x < 1000 {
		fmt.Println("Você receberá um aumento de salario de 10% acrecentando: ", x*0.1)
	} else {
		fmt.Println("Você receberá um aumento de salario de 5% acrecentando:", x*0.05)
	}

}

Algoritimo 8º seleção:



Algoritimo 1º repetição:

package main

import "fmt"

func main() {
	var i int64
	for i = 0; i <= 10; i++ {
		fmt.Println("O número é", i)
	}
}

Algoritimo 2º repetição:

package main

import "fmt"

func main() {
	var i int64
	for i = 0; i <= 20; i++ {
		if i%2 == 0 {
			fmt.Println("Os números pares até 20:", i)
		}
	}
}

Algoritimo 3º repetição:

package main

import "fmt"

func main() {
	var i int64
	for i = 1; i <= 19; i++ {
		if i%2 == 1 {
			fmt.Println("Os números impares até 20:", i)
		}
	}
}

Algoritimo 4º repetição:

package main

import "fmt"

func main() {
	var i int64
	for i = 0; i <= 30; i++ {
		if i%3 == 0 {
			fmt.Println("Os números multiplos de 3 até 30 são:", i)
		}
	}
}

Algoritimo 5º repetição:

package main

import "fmt"

func main() {
	var i int64
	for i = 10; i <= 0; i-- {
		fmt.Println("O número é", i)
	}
}

