import "fmt"

func main() {
	var x int64
	var y int64
	fmt.Print("Digite Os dias trabalhados! ")
	fmt.Scan(&x)
	fmt.Print("Digite o salário em dias! ")
	fmt.Scan(&y)
	fmt.Println("O salario mensal é de: ", x*y)
}

